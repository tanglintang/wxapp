<view class="page">
    <view class="weui-cells">
        <navigator class="weui-cell weui-cell_access head" url="/pages/home/auth/auth" hover-class="weui-cell_active">
            <view class="weui-cell__hd">
                <image src="{{userInfo.avatar}}" />
            </view>
            <view class="weui-cell__bd">
                <view class="name">{{userInfo.nickName}}</view>
                <view class="stu_id">{{stu_id}}</view>
            </view>
            <view class="weui-cell__ft  weui-cell__ft_in-access"></view>
        </navigator>
    </view>
    <view class="weui-cells" wx:if="{{userInfo.isBindEdu}}">
        <navigator class="weui-cell weui-cell_access" hover-class="weui-cell_active" url="/pages/score/score">
            <view class="weui-cell__hd">
                <image src="../../images/icon_score.png"></image>
            </view>
            <view class="weui-cell__bd">90</view>
            <view class="weui-cell__ft  weui-cell__ft_in-access"></view>
        </navigator>
    </view>
    <view class="weui-cells">
        <navigator url="../card/card" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
            <view class="weui-cell__hd">
                <image src="../../images/icon_card.png"></image>
            </view>
            <view class="weui-cell__bd">校园卡</view>
            <view class="weui-cell__ft weui-cell__ft_in-access"></view>
        </navigator>
        <navigator url="../electric_card/electric" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
            <view class="weui-cell__hd">
                <image src="../../images/icon_electric.png"></image>
            </view>
            <view class="weui-cell__bd">电卡</view>
            <view class="weui-cell__ft weui-cell__ft_in-access"></view>
        </navigator>
    </view>
    <view class="weui-cells">
        <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
            <view class="weui-cell__hd">
                <image src="../../images/icon_catalog.png"></image>
            </view>
            <view class="weui-cell__bd">登记</view>
            <view class="weui-cell__ft weui-cell__ft_in-access"></view>
        </navigator>
    </view>
    <view class="weui-cells">
        <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
            <view class="weui-cell__hd">
                <image src="../../images/icon_service.png"></image>
            </view>
            <view class="weui-cell__bd">服务</view>
            <view class="weui-cell__ft weui-cell__ft_in-access"></view>
        </navigator>
        <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
            <view class="weui-cell__hd">
                <image src="../../images/icon_setting.png"></image>
            </view>
            <view class="weui-cell__bd">设置</view>
            <view class="weui-cell__ft weui-cell__ft_in-access"></view>
        </navigator>
    </view>
</view>